{
  "name": "vibha-stock-alerts-dual-market-server",
  "version": "2.0.0",
  "description": "Dual-Market Backend Server for Vibha Stock Alerts - Indian (BSE/NSE) + Global (NYSE/NASDAQ) Markets",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:indian": "ENABLE_GLOBAL_MARKETS=false nodemon server.js",
    "dev:global": "ENABLE_INDIAN_MARKETS=false nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:markets": "node scripts/test-markets.js",
    "health-check": "curl http://localhost:3001/api/health",
    "cache-clear": "node scripts/clear-cache.js"
  },
  "keywords": [
    "stock", 
    "api", 
    "alerts", 
    "express", 
    "BSE", 
    "NSE", 
    "NYSE", 
    "NASDAQ", 
    "indian-markets", 
    "global-markets",
    "dual-market",
    "alpha-vantage"
  ],
  "author": "Vibha StockAlerts Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vibha-stock-alerts.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vibha-stock-alerts/issues"
  },
  "homepage": "https://vibhastockalerts.com"
}
